{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "module": "ESNext",
    "target": "ESNext",
    "lib": [
      "DOM",
      "ESNext",
      "WebWorker"
    ],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "incremental": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "jsx": "preserve",
    "resolveJsonModule": true,
    "allowJs": true,
    "noUnusedLocals": false,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noUnusedParameters": true,
    "isolatedModules": true,
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "importHelpers": true,
    "typeRoots": [
      "./node_modules/@types/",
      "./src/types",
      "vite/types"
    ],
    "types": [
      "node"
    ],
    "paths": {
      "@/*": [
        "src/*"
      ],
      "~/*": [
        "src/*"
      ],
      "~c/*": [
        "./src/core/composables/*"
      ],
      "~co/*": [
        "./src/core/*"
      ],
      "~h/*": [
        "./src/core/helpers/*"
      ],
      "~cf/*": [
        "./src/config/*"
      ],
      "~m/*": [
        "./src/modules/*"
      ],
      "~t/*": [
        "./src/themes/*"
      ]
    },
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo"
  }
}
